{
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:prettier/recommended",
    "plugin:jest/recommended"
  ],
  "plugins": ["prettier", "import", "jest"],
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true
    }],
    "linebreak-style": ["error"],
    "quotes": ["error", "single"]
  },
  "env": {
    "node": true,
    "jest/globals": true
  }
}